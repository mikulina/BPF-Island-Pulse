{
  "name": "island-pulse",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "10.13.0"
  },
  "scripts": {
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "build": "bower install && docker-compose -f docker-compose.yml build",
